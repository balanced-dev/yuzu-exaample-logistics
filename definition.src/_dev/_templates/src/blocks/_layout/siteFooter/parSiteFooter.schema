{
    "id": "/parSiteFooter",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "",
    "type": "object",
    "properties": {
        "linkSections": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "title": {
                        "type": "string"
                    },
                    "links": {
                        "type": "array",
                        "items": {
                            "$ref": "/parDataLink"
                        }
                    }
                }
            }
        },
        "socialSection": {
            "type": "object",
            "properties": {
                "title": {
                    "type": "string"
                },
                "links": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "type": {
                                "type": "string"
                            },
                            "link": {
                                "$ref": "/parDataLink"
                            }
                        }
                    }
                }
            }
        },
        "newsletterSection": {
            "type": "object",
            "properties": {
                "title": {
                    "type": "string"
                },
                "form": {
                    "$ref": "/parDataForm",
                    "anyOfType": "parFormFooter"
                }
            }
        }
    },
    "additionalProperties": false
}